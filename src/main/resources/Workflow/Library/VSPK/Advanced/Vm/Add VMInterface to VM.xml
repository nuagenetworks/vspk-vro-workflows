<?xml version="1.0" encoding="UTF-8"?>
<workflow xmlns="http://vmware.com/vco/workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://vmware.com/vco/workflow http://vmware.com/vco/workflow/Workflow-v4.xsd" root-name="item1" object-name="workflow:name=generic" id="d3d3b4da-afba-4421-8ca5-4a6bf5d29e78" version="1.0.0" api-version="6.0.0" allowed-operations="vef" restartMode="1" resumeFromFailedMode="0">
  <display-name><![CDATA[Add VMInterface to VM]]></display-name>
  <ref-types>><![CDATA[(VSPK:VM)(VSPK:VPort)]]></ref-types>>
  <position y="50.0" x="100.0"></position>
  <input>
    <param name="vm" type="VSPK:VM"></param>
    <param name="interfaceName" type="string"></param>
    <param name="mac" type="string"></param>
    <param name="ipAddress" type="string"></param>
    <param name="vport" type="VSPK:VPort"></param>
    <param name="externalID" type="string"></param>
  </input>
  <output>
    <param name="vmInterface" type="VSPK:VMInterface"></param>
  </output>
  <workflow-item  name="item0" type="end" end-mode="0">
    <position y="45.40909090909091" x="384.5"></position>
  </workflow-item >
  <workflow-item  name="item1" out-name="item0" type="task">
    <display-name><![CDATA[Scriptable task]]></display-name>
    <script encoded="false"><![CDATA[var session = vport.session;

vmInterface = new VSPKVMInterface();
if (interfaceName) {
   vmInterface.name = interfaceName;
}
if (mac) {
   vmInterface.MAC = mac;
}
if (ipAddress) {
   vmInterface.IPAddress = ipAddress;
}
if (vport) {
   vmInterface.VPortID = vport.id;
}
if (externalID) {
   vmInterface.externalID = externalID;
}

vm.createVMInterface(session, vmInterface);]]></script>
    <in-binding>
      <bind name="vport" type="VSPK:VPort" export-name="vport"></bind>
      <bind name="vm" type="VSPK:VM" export-name="vm"></bind>
      <bind name="interfaceName" type="string" export-name="interfaceName"></bind>
      <bind name="mac" type="string" export-name="mac"></bind>
      <bind name="ipAddress" type="string" export-name="ipAddress"></bind>
      <bind name="externalID" type="string" export-name="externalID"></bind>
    </in-binding>
    <out-binding>
      <bind name="vmInterface" type="VSPK:VMInterface" export-name="vmInterface"></bind>
    </out-binding>
    <position y="55.40909090909091" x="204.5"></position>
  </workflow-item >
