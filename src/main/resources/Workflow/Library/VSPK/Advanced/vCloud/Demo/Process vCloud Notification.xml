  <workflow-item name="item0" out-name="item9" type="task">
    <display-name><![CDATA[Extract Notification fields]]></display-name>
    <script encoded="false"><![CDATA[var notificationHelper = new VclNotificationHelper();
notificationHelper.setMessage(messageBody);

var organizationLink = notificationHelper.getOrgLink();
orgEntityId = organizationLink.id;
System.log("organization entity ID: " + orgEntityId);

var notification = notificationHelper.getNotification();
var notificationType = notification.type;
var notificationTypeParts = notificationType.split("/");
actionType = notificationTypeParts[notificationTypeParts.length - 1];

var entityLink = notificationHelper.getEntityLink();
entityType = entityLink.type;
entityId = entityLink.id;
entityName = entityLink.name;

operationSuccess =  notification.operationSuccess;
]]></script>
