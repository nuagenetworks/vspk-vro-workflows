<?xml version="1.0" encoding="UTF-8"?>
<workflow xmlns="http://vmware.com/vco/workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://vmware.com/vco/workflow http://vmware.com/vco/workflow/Workflow-v4.xsd" root-name="item4" object-name="workflow:name=generic" id="cade4506-2a06-4d05-8b68-af0118509c10" version="1.0.0" api-version="6.0.0" allowed-operations="vef" restartMode="1" resumeFromFailedMode="0">
  <display-name><![CDATA[Delete VM and associated VPorts]]></display-name>
  <position y="50.0" x="100.0"></position>
  <input>
    <param name="vm" type="VSPK:VM"></param>
  </input>
  <attrib name="vPortIds" type="Array/string" read-only="false">
    <value encoded="n"><![CDATA[__NULL__]]></value>
